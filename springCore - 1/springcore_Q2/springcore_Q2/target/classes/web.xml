<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context=" http://www.springframework.org/schema/context"
 xmlns:p="http://www.springframework.org/schema/p"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 
 
 
  <bean class="com.springcore.collections.List_Q2" name="Listname">
  
   <property name="id" >
  <value>1</value>
  </property>
 
  <property name="username">
  <list>
  <value>Mia</value>
  <value>James</value>
  <value>Tom</value>
  </list>
  </property>
 
 </bean>
 
 
 <bean class="com.springcore.collections.Set_Q2" name="Setname">
  
   <property name="id" >
  <value>2</value>
  </property>
  
  <property name="username">
  <set>
  <value>John</value>
  <value>Billy</value>
  <value>Sam</value>
  </set>
  </property>
 </bean>
 
 
 <bean class="com.springcore.collections.Map_Q2" name="Mapname">
  
   <property name="id" >
  <value>3</value>
  </property>
  
  <property name="username">
  <map>
  <entry key="1" value="Starck"/>
  <entry key="2" value="Smith"/>
  <entry key="3" value="Lyon"/>
  
  </map>
  </property>
 
 </bean>
 </beans>