<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context=" http://www.springframework.org/schema/context"
 xmlns:p="http://www.springframework.org/schema/p"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 
 
 
  <bean class="com.springcore_Q1.Address" name="address1">
  <property name="street" >
  <value>reddyColony</value>
  </property>
  
  <property name="city">
  <value>hanamkonda</value>
  </property>
  
   <property name="zip">
  <value>5647865</value>
  </property>
  
   <property name="state">
  <value>Telengana</value>
  </property>
  
   <property name="country">
  <value>india</value>
  </property>
 
 </bean>
 
 
 <bean class="com.springcore_Q1.Customer" name="customer1">
<property name="customerId">
  <value>1</value>
  </property>
  
  <property name="customerName">
  <value>Pavan</value>
  </property>
  
  <property name="customerContact">
  <value>950</value>
  </property>

 <property name="customerAddress">
  <ref bean="address1"/>

  </property>
 </bean>
 </beans>